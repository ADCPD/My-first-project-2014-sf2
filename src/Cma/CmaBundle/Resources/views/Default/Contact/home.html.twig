
<div class="span8">
    <div class="footercontent">
        <h4 class="footertitle">{{ contact.title }}</h4>
        <div class="map">
            <iframe src="{{ contact.map }}" width="100%" height="100%" style="position: relative;">	</iframe>
{#width="245px" height="180px"#}
        </div>
        <div class="address" style="margin-left: 20px;">
            <adresse>
                <h5>Adresse Postale</h5>
                <p>
                    Adresse. {{ contact.postalAdress }}
                    <br/>
                    Mail. {{ contact.mail }}
                    <br/>
                    Portable. {{ contact.phone }}
                    <br/>
                    Fax. {{ contact.fax }}
                </p>
            </adresse>    

        </div>


    </div>
</div>
